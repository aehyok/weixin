<swiper indicator-dots="false" autoplay="true" interval="2000" duration="1000" style="height:{{screenHeight}}">
  <block wx:for="{{imgUrls}}" wx:key="{{ item }}">
    <swiper-item>
      <image src="{{item}}" class="slide-image"/>
      <button class="btn" bindtap="start">立即体验</button>
    </swiper-item>
  </block> 
</swiper>
